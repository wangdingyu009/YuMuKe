/* pages/supplytask/supplytask.wxss */
page {
  background: #F5F5F6;
}

.supply {
  padding-left: 32rpx;
  padding-right: 32rpx;
  /* padding-top: 24rpx; */
}

.head {
  padding-left: 32rpx;
  height: 56rpx;
  padding-left: 32rpx;
  padding-top: 14rpx;
  padding-bottom: 18rpx;
  background: #fff;
  margin-bottom: 24rpx;
  display: flex;
  justify-content: start;
}

.head .pickermethod {
  width: 206rpx;
}

.head .week-l, .head .week-r {
  position: relative;
  min-width: 206rpx;
  padding-left: 32rpx;
  margin-right: 16rpx;
  /* padding-right: 64rpx; */

  height: 54rpx;
  border-radius: 27rpx;
  line-height: 54rpx;
  border: 1rpx solid #CCCCCF;
}

.week-l image, .week-r image {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  right: 16rpx;
  top: 3rpx;
}

.list {
  width: 638rpx;
  padding: 32rpx 24rpx;
  background: #fff;
  margin-bottom: 16rpx;
}

.date {
  display: flex;
  justify-content: space-between;
}

.date .left {
  font-size: 40rpx;
  color: #333336;
  font-weight: 600;
}

.overdue .date .left {
  color: #CCCCCF;
}

.date .left text {
  font-size: 28rpx;
  font-weight: 400;
}

.date .right {
  height: 52rpx;
  line-height: 52rpx;
  background: #EBF0FF;
  border-radius: 10rpx;
  font-size: 32rpx;
  color: #5D88FF;
  padding-left: 24rpx;
  padding-right: 24rpx;
}

.overdue .date .right {
  color: #fff;
  background: #D8D8DB;
}

.desc {
  font-size: 32rpx;
  color: #333336;
  margin-top: 24rpx;
  margin-bottom: 16rpx;
}

.overdue .desc {
  color: #CCCCCF;
}

.room {
  font-size: 28rpx;
  color: #666669;
}
.overdue .room {
  color: #CCCCCF;
}