<!--pages/messagedetial/index.wxml-->
<view class="message">
  <view class="title">
    {{detail.subject}}
  </view>
  <view class="time">{{detail.creationtime}}</view>
  <view class="content">
    {{detail.content}}
  </view>
</view>
<view wx:for="{{detail.attachments}}" data-name="{{item.fileName}}" data-url="{{item.fileUrl}}" wx:key="index" bindtap="downFile" class="file">
  <view class="label">附件</view>
  <view class="file-url">{{item.fileName}}</view>
</view>
