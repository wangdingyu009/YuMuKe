/* pages/account/index.wxss */
page {
  background-color: #F5F5F6;
}
.user-info {
  background-color: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 40rpx 0;
  
}
.user-info image {
  width: 128rpx;
  height: 128rpx;
  border-radius: 64rpx;
}
.user-info .name{
  font-weight: 600;
  color: #333336;
  line-height: 48rpx;
  margin-top: 24rpx;
  font-size: 32rpx;
}
.account-list {
  background-color: #fff;
  padding: 0 32rpx;
  margin-top: 16rpx;
}
.account-list-item {
  padding: 30rpx 0;
  color: #333336;
  line-height: 48rpx;
  font-size: 32rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.account-list-item image {
  width: 48rpx;
  height: 48rpx;
}