<!--pages/account/index.wxml-->
<view class="account">
  <view class="user-info">
    <image src="/images/touxiang.png"></image>
    <view class="name">张小碗</view>
  </view>
  <view class="account-list">
    <view class="account-list-item" bindtap="photoTap">
      <text>照片</text>
      <image src="/images/more.png"></image>
    </view>
    <view class="account-list-item">
      <text>学号</text>
      <view>201046840210</view>
    </view>
    <view class="account-list-item">
      <text>院系</text>
      <view>新闻与传播学院</view>
    </view>
    <view class="account-list-item">
      <text>专业</text>
