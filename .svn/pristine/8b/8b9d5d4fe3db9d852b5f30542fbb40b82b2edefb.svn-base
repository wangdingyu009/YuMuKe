    </view>
    <view class="account-list-item">
      <text>专业方向</text>
      <view>生物科学院士</view>
    </view>
    <view class="account-list-item">
      <text>年级</text>
      <view>2年级</view>
    </view>
    <view class="account-list-item">
      <text>班级</text>
      <view>广告2班</view>
    </view>
  </view>
  <view class="account-list">
    <view class="account-list-item" bindtap="changePassword" style="margin-top: 16rpx;">
      <text>修改密码</text>
      <image src="/images/more.png"></image>
    </view>
  </view>
  
</view>
