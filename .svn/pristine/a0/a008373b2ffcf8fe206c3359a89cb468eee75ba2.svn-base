<!--pages/info/info.wxml消息页-->
<view class="info">
  <view class="lines"></view>
  <view class="layer">
    <view wx:if="{{list.length > 0}}" wx:for="{{list}}" wx:key="index" class="list-item" data-id="{{item.MESSAGEID}}" bindtap="detialTap">
      <image wx:if="{{messageType == '2'}}" class="item-l" src="/images/gonggao.png"></image>
      <image wx:if="{{messageType == '0'}}" class="item-l" src="/images/tongzhi.png"></image>
      <image wx:if="{{messageType == '1'}}" class="item-l" src="/images/xitong.png"></image>
      <view class="item-r">
        <view class="top">
          <view wx:if="{{messageType == '2'}}" class="top-l">公告</view>
          <view wx:if="{{messageType == '0'}}" class="top-l">通知</view>
          <view wx:if="{{messageType == '1'}}" class="top-l">系统</view>
          <view class="top-r">{{itme.SHOWDATE}}</view>
        </view>
        <view class="bottom">
          <view class="bottom-l">
            {{item.SUBJECT}}
          </view>
          <!-- <view class="bottom-r">9</view> -->
        </view>
      </view>
    </view>

  </view>
</view>

