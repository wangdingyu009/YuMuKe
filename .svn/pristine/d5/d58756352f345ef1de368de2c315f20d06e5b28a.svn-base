<!--pages/Feedback/index.wxml-->
<view class="feedback">
  <view class="feedback-box">
    <textarea maxlength="200" value="{{value}}" placeholder-style="placeholder-class" bindinput="bindKeyInput" placeholder="请输入您的宝贵意见" />
    <view class="num">{{currentNum}}/{{maxNum}}</view>
  </view>
  <view class="pass-input">
    <view class="label">您的联系方式</view>
    <input confirm-type="done" placeholder-style="placeholder-class" bindinput="telChange" placeholder="请输入您的联系方式" value="{{phone}}"/>
  </view>
  <view bindtap="submit" class="btn">提交</view>
</view>
