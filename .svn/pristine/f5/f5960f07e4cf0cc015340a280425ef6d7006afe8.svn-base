/* pages/roomapprove/roomapprove.wxss */
/* 头部下拉框start */
page {
  background: #F5F5F6;
}

.head {
  padding-left: 32rpx;
  height: 56rpx;
  padding-left: 32rpx;
  padding-right: 32rpx;
  padding-top: 14rpx;
  padding-bottom: 18rpx;
  background: #fff;
  margin-bottom: 24rpx;
  display: flex;
  justify-content: space-between;
}

.head .pickermethod {
  width: 206rpx;
}

.head .week-l,
.head .week-r {
  position: relative;
  width: 173rpx;
  padding-left: 33rpx;
  font-weight: 500;

  height: 50rpx;
  border-radius: 27rpx;
  line-height: 50rpx;
  border: 1rpx solid #CCCCCF;
}

.week-l image,
.week-r image {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  right: 4rpx;
  top: 0rpx;
}
/* 头部下拉框end */
/* 列表start */
.list {
  padding-left: 32rpx;
  padding-right: 32rpx;
}

.layer {
  width: 638rpx;
  height: 140rpx;
  position: relative;
  padding: 24rpx;
  border-radius: 8rpx;
  background: #fff;
  font-size: 28rpx;
  color: #666669;
  margin-top: 16rpx;
}

.layer view {
  width: 100%;
  overflow: hidden;
  height: 40rpx;
}

.item-1 {
  height: 56rpx !important;
  overflow:hidden; 
  text-overflow:ellipsis; 
  white-space:nowrap; 
  font-size: 40rpx;
  font-weight: 600;
  color: #333336;
  margin-bottom: 4rpx;
}

.item-5 {
  position: absolute;
  right: 0;
  bottom: 0;
  border-radius: 28rpx 0px 8rpx 0px;
  width: 92rpx !important;
  height: 32rpx !important;
  background: #6CCB85;
  text-align: center;
  color: #fff;
  font-size: 20rpx;
}

.red {
  background: #FF5D5D;
}

.blue {
  background: #5D88FF;
}

