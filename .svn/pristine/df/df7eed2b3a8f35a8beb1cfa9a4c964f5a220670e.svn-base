/* pages/switchapply/switchapply.wxss */
page {
  background: #F5F5F6;
}

.head {
  padding-left: 32rpx;
  height: 56rpx;
  padding-left: 32rpx;
  padding-top: 14rpx;
  padding-bottom: 18rpx;
  background: #fff;
  margin-bottom: 24rpx;
  display: flex;
  justify-content: start;
}

.head .pickermethod {
  width: 206rpx;
}

.head .week-l,
.head .week-r {
  position: relative;
  min-width: 206rpx;
  padding-left: 32rpx;
  margin-right: 16rpx;
  /* padding-right: 64rpx; */

  height: 54rpx;
  border-radius: 27rpx;
  line-height: 54rpx;
  border: 1rpx solid #CCCCCF;
}

.week-l image,
.week-r image {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  right: 16rpx;
  top: 3rpx;
}

.apply {
  padding-left: 32rpx;
  padding-right: 32rpx;
}

.list {
  height: 196rpx;
  padding: 24rpx;
  font-size: 28rpx;
  color: #666669;
  background: #fff;
  margin-bottom: 16rpx;
}

.title {
  font-size: 40rpx;
  color: #333336;
  font-weight: 600;
  line-height: 60rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.reason {
  line-height: 48rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.time {
  line-height: 44rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.type {
  line-height: 44rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}