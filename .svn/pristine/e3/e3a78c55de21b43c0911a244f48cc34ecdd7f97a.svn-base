/* pages/roomapply/roomapply.wxss */
/* 头部下拉框start */
page {
  background: #F5F5F6;
}

.head {
  position: sticky;
  top: 0;
  z-index: 99;
  padding-left: 32rpx;
  height: 56rpx;
  padding-left: 32rpx;
  padding-right: 32rpx;
  padding-top: 14rpx;
  padding-bottom: 18rpx;
  background: #fff;
  margin-bottom: 24rpx;
  display: flex;
  justify-content: space-between;
}

.head .pickermethod {
  width: 206rpx;
}

.head .week-l,
.head .week-r {
  position: relative;
  /* min-width: 206rpx; */
  width: 173rpx;
  /* margin-right: 16px; */
  padding-left: 20rpx;
  /* padding-left: 32rpx; */
  /* padding-right: 64rpx; */

  height: 54rpx;
  border-radius: 27rpx;
  line-height: 54rpx;
  border: 1rpx solid #CCCCCF;
}

.week-l image,
.week-r image {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  right: 4rpx;
  top: 0rpx;
}
/* 头部下拉框end */
/* 列表start */
.list {
  padding-left: 32rpx;
  padding-right: 32rpx;
}
.layer {
  position: relative;
  width: 638rpx;
  padding: 24rpx;
  height: 106rpx;
  background: #fff;
  border-radius: 8rpx;
  margin-top: 16rpx;
}

.item-t {
  height: 56rpx;
  overflow:hidden; 
  text-overflow:ellipsis; 
  white-space:nowrap; 
  font-size: 40rpx;
  font-weight: 600;
  color: #333336;
}

.item-b {
  font-size: 28rpx;
  font-weight: 400;
  color: #666669;
  margin-top: 10rpx;
}

.apply-status {
  position: absolute;
  right: 0;
  bottom: 0;
  border-radius: 28rpx 0px 8rpx 0px;
  width: 92rpx;
  height: 32rpx;
  background: #6CCB85;
  text-align: center;
  color: #fff;
  font-size: 20rpx;
}

.red {
  background: #FF5D5D;
}

.blue {
  background: #5D88FF;
}