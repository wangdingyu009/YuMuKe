/* pages/currentsemester/currentsemester.wxss */
.current {
  width: 686rpx;
  min-height: 100vh;
  background: #F5F5F6;
  font-size: 32rpx;
  color: #333336;
  font-weight: 400;
  padding: 0rpx 32rpx;
}

.content {
  width: 622rpx;
  background: #FFF;
  padding: 24rpx 32rpx;
  margin-top: 16rpx;
}

.content .title {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.content .number {
  color: #666669;
  font-size: 28rpx;
  margin-top: 16rpx;
}