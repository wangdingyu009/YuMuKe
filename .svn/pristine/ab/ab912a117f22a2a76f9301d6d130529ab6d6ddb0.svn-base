/* pages/roomselect/roomselect.wxss */
page {
  background: #F5F5F6;
}

.layer {
  background: #fff;
  margin-top: 16rpx;
}

.select {
  font-size: 32rpx;
  color: #333336;
}

.search {
  position: relative;
  /* background: #fff; */
  height: 60rpx;
  padding-top: 14rpx;
  padding-bottom: 14rpx;
  padding-left: 24rpx;
  padding-right: 24rpx;
  border-bottom: 1rpx solid #EEEEF1;
}

.search input {
  width: 492rpx;
  height: 54rpx;
  border: 2rpx solid #5D88FF;
  border-radius: 12rpx;
  padding-left: 60rpx;
  padding-right: 54rpx;
}

.sousuo {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  top: 27rpx;
  left: 48rpx;
}

.close {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 28rpx;
  right: 130rpx;
}

.set-bt {
  border: none;
  width: 80rpx !important;
  padding: 0 !important;
  line-height: 88rpx !important;
  color: #5D88FF !important;
  font-weight: 400 !important;
  font-size: 30rpx !important;
  position: absolute !important;
  right: 10rpx !important;
  top: -2rpx !important;
  background: #fff !important;
  /* left: 10rpx !important; */
}

.list {
  padding-left: 32rpx;
  padding-right: 32rpx;
}

.room-name {
  height: 104rpx;
  line-height: 104rpx;
  font-size: 32rpx;
  color: #333336;
  border-bottom: 1rpx solid #EEEEF1;
}