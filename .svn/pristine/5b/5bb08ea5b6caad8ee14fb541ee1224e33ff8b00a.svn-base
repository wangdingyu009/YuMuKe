/* pages/studywarn/studywarn.wxss */
page {
  background: #F5F5F6;
}

.layer {
  padding-left: 32rpx;
  padding-right: 32rpx;
}

.list {
  border-radius: 16rpx;
  padding: 28rpx;
  background: #fff;
  display: flex;
  justify-content: start;
  margin-top: 16rpx;
}

.list .left {
  width: 62rpx;
}

.list .left image {
  width: 48rpx;
  height: 48rpx;
}

.list .right {
  width: 568rpx;
}

.list .right .title {
  font-size: 32rpx;
  line-height: 44rpx;
  font-weight: 500;
  color: #333336;
}

.list .right .time {
  font-size: 24rpx;
  color: #99999C;
  line-height: 36rpx;
}

.list .right .desc {
  font-size: 28rpx;
  color: #333336;
  line-height: 44rpx;
}