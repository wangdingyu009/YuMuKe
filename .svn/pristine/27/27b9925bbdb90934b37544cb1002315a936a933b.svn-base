Page({

  /**
   * 页面的初始数据
   */
  data: {
    zhouci: ['第一周','第二周','第三周','第四周','第五周','第六周','第七周'],
    zhouciIndex: 0,
    xingqi: ['星期一','星期二','星期三','星期四','星期五','星期六','星期日'],
    xingqiIndex: 0,
    zhuangtai: ['全部', '待审批', '已通过', '已拒绝'],
    zhuangtaiIndex: 0,
  },

  goDesc() {
    wx.navigateTo({
      url: '/pages/switchapplyinfo/switchapplyinfo',
    })
  },

  zhuangtaiChange(e) {
    this.setData({
      zhuangtaiIndex: e.detail.value
    })
  },

  xingqiChange(e) {
    this.setData({
      xingqiIndex: e.detail.value
    })
  },
  
  zhouciChange(e) {
    this.setData({
      zhouciIndex: e.detail.value
    })
  },

  /**
   * 生命周期函数--监听页面加载
   */
