/* pages/gradeexamination/gradeexamination.wxss */
.grade {
  min-height: 100vh;
  background: #F5F5F6;
}

.date {
  height: 88rpx;
  background: #fff;
}

.layer {
  padding-left: 32rpx;
  padding-right: 32rpx;
  margin-top: 24rpx;
}

.time-table {
  margin-top: 16rpx;
  background: #fff;
  width: 622rpx;
  min-height: 96rpx;
  padding: 30rpx 28rpx;
  padding-right: 32rpx;
  border-left: 4rpx solid #FFAB5C;
  display: flex;
  justify-content: start;
}

.time-table .left {
  width: 457rpx;
  padding-right: 32rpx;
  color: #333336;
  font-size: 32rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.time-table .left .title {
  /* margin-bottom: 8rpx; */
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.time-table .left .time {
  font-size: 28rpx;
  color: #99999C;
}

.time-table .right {
  width: 132rpx;
  /* line-height: 96rpx; */
  border-left: 1rpx solid #D8D8D8;
  font-size: 48rpx;
  color: #5D88FF;
  font-weight: 500;
  text-align: center;
  display: flex;
  align-items: center;
  /* flex-direction: column; */
}