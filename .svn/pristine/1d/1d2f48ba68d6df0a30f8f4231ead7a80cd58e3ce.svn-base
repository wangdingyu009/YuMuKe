/* pages/infotypelist/infotypelist.wxss */
.info {
  min-height: 100vh;
  background: #fff;
}
.lines {
  width: 750rpx;
  background: #F5F5F6;
  height: 12rpx;
}
.layer {
  padding-left: 32rpx;
  padding-right: 32rpx;
}
.list-item {
  width: 686rpx;
  height: 92rpx;
  display: flex;
  justify-content: space-between;
  margin-top: 48rpx;
}
.item-l {
  width: 92rpx;
  height: 92rpx;
}
.item-r {
  width: 562rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.top {
  font-size: 32rpx;
  color: #333336;
  font-weight: 600;
  display: flex;
  justify-content: space-between;
}
.top-l {
  width: 358rpx;
}
.top-r {
  width:204rpx;
  text-align: right;
  font-size: 24rpx;
  font-weight: 400;
  color: #99999C;
}
.bottom {
  font-size: 28rpx;
  font-weight: 400;
  color: #99999C;
  display: flex;
  justify-content: space-between;
}
.bottom-l {
  width: 510rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.bottom-r {
  width: 52rpx;
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
  font-size: 24rpx;
  border-radius: 19rpx;
  color: #FFF;
  background: #FF5D5D;
  margin-top: 4rpx;
}