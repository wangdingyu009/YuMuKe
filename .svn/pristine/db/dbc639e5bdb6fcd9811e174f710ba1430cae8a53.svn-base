Page({

  /**
   * 页面的初始数据
   */
  data: {

  },

  /**
   * 生命周期函数--监听页面加载
   */
  onLoad: function (options) {

  },
  jumpTap: function (e) {
    console.log(e)
    let type = e.currentTarget.dataset.type
    if(type == 1) {
      wx.navigateTo({
        url: '/pages/account/index', // 我的账户
      })
    } else if(type == 4) {
      wx.navigateTo({
        url: '/pages/contactus/index', // 联系我们
      })
    }
  },
  /**
   * 生命周期函数--监听页面初次渲染完成
   */
