<!--pages/info/info.wxml消息页-->
<view class="info">
  <view class="lines"></view>
  <view class="layer">
    <view class="list-item" bindtap="detialTap">
      <image class="item-l" src="/images/gonggao.png"></image>
      <view class="item-r">
        <view class="top">
          <view class="top-l">公告</view>
          <view class="top-r">{{message2.showDate}}</view>
        </view>
        <view class="bottom">
          <view class="bottom-l">
            {{message2.subject}}
          </view>
          <!-- <view class="bottom-r">9</view> -->
        </view>
      </view>
    </view>
    <view class="list-item" bindtap="detialTap">
      <image class="item-l" src="/images/tongzhi.png"></image>
      <view class="item-r">
        <view class="top">
          <view class="top-l">通知</view>
          <view class="top-r">{{message0.showDate}}</view>
        </view>
        <view class="bottom">
          <view class="bottom-l">
            {{message0.subject}}
          </view>
          <!-- <view class="bottom-r">9</view> -->
        </view>
      </view>
    </view>
    <view class="list-item" bindtap="detialTap">
      <image class="item-l" src="/images/xitong.png"></image>
      <view class="item-r">
        <view class="top">
          <view class="top-l">系统</view>
          <view class="top-r">{{message1.showDate}}</view>
        </view>
        <view class="bottom">
          <view class="bottom-l">
            {{message1.subject}}
          </view>
          <!-- <view class="bottom-r">9</view> -->
        </view>
      </view>
    </view>

  </view>
</view>
